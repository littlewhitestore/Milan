.swiper-tab {
  width: 100%;
  /* border-bottom: 1rpx solid rgba(151, 159, 167, 0.5); */
  text-align: center;
  height:100rpx;
  line-height: 100rpx;
  background-color:  #fafbfc;
  display:flex;
  flex-direction:row;
  justify-content:space-around;
}


.swiper-tab-list {
  font-size: 14px;
  
  color: #48434c;
}

.on {
  color: #f33975;
  /* border-bottom: 5rpx solid#f33975; */
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}

.swiper-box view {
  margin-top: 10rpx;
}

.list {
  display: flex;
  flex-direction: column;
  font-size: 14px;
}

.num {
  padding: 10rpx 30rpx;
  height: 58rpx;
  line-height: 58rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.no {
  color: #43484c;
}

.status {
  color: #43484c;
}

.line {
  border-bottom: solid 1rpx rgba(151, 159, 167, 0.5);
  margin-bottom: 10rpx;
}

.liner {
  margin-top: 20rpx;
  border-bottom: dashed 1rpx rgba(151, 159, 167, 0.5);
}

.con {
  padding: 10rpx 0 10rpx 30rpx;
  height: 210rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.time {
  padding-left: 40rpx;
  height: 40rpx;
  font-size: 14px;
  color: #48434c;
}

.img {
  height: 200rpx;
  width: 200rpx;
}

.font {
  display: flex;
  flex-direction: column;
  width: 480rpx;
  margin: 0rpx 30rpx;
}

.name {
  font-size: 14px;
  color: #979fa7;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.number {
  color: #979fa7;
  font-size: 14px;
  padding-top: 30rpx;
}

.price {
  padding-top: 20rpx;
  font-size: 14px;
  color: #757f8a;
}

.bottom {
  /* margin: 10rpx 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  font-size: 14px;
  color: #979fa7; */
  margin: 28rpx 0;
  width: 750rpx;
  height: 15rpx;
  background-color: #fafbfc;
}

.share image {
  width: 46rpx;
  height: 46rpx;
  margin-right: 4rpx;
}

.o {
  margin: 260rpx auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
  align-items: center;
}

.nofont {
  margin-top: 30rpx;
  color: #43484c;
  font-size: 14px;
}

.noimg {
  width: 160rpx;
  height: 140rpx;
}

.btn {
  padding: 4rpx 6rpx 4rpx 2rpx;
  width: 150rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #fe6e1f;
  color: #fff;
  font-size: 14px;
  border-radius: 5rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.btn::after {
  border: 0;
}

.space {
  height: 20rpx;
  background-color: #f7f7f7;
}

.gohome {
  width: 378rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 80rpx;
  background-color: #f33975;
  border-radius: 5rpx;
  color: #fff;
  border: 0;
  font-size: 14px;
}

.gohome::after {
  border: 0;
}

.base {
  width: 750rpx;
  height: 20rpx;
  background-color: #e9eaec;
}

